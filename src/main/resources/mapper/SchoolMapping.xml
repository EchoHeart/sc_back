<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.sc_back.dao.SchoolDao">
    <select id="getSchoolByName" resultType="com.example.sc_back.bean.School">
        SELECT * FROM school WHERE school_name = #{school_name};
    </select>

    <insert id="setSchoolByRegister">
        INSERT INTO school(school_name) VALUES (#{school_name});
    </insert>
</mapper>
